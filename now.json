{
    "version": 2,
    "name": "slovenskymanazer",
    "alias": [
      "slovenskymanazer-app"
    ],
    "env": {
      "bucket_name": "@bucket_name",
      "access_key": "@access_key",
      "secret_key": "@secret_key",
      "region": "@region",
      "mongodb_uri": "@my-mongodb-uri"
    },
    "routes": [
      {"src": "/", "methods":["GET", "POST"], "dest": "index.html"}
    ]
  }
